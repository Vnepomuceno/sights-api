{ "_id" : { "$oid" : "59c43d29dd5e3d0a02433b50" }, "labels" : [ { "term" : "primary", "value" : "Miradouro da Torre de Belém", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "term" : "note", "value" : "SG-DMC", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "descriptions" : [ { "value" : "Do terraço da torre tem-se ampla vista do estuário do Tejo e das suas margens, bem como de toda a zona de Belém e seus monumentos. De costas para o rio avista-se a Capela de São Jerónimo e o Mosteiro dos Jerónimos, ainda que parcialmente encoberto pelo Centro Cultural de Belém.", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "images" : [ { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/torre-belem_AS36006_01.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/torre-belem_ASR0416_01.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "websites" : [ { "href" : "www.torrebelem.pt/", "type" : "text/html", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "contact" : { "phoneNumber" : "+351 213 620 034", "emailAddress" : "mjeronimos@igespar.pt" }, "schedules" : [ { "open" : "out-abr: 10h00 - 17h30 (última entrada às 17h00); maio-set: 10h00 - 18h30 (última entrada às 18h00)<br />encerra: 2ª f, 1 de janeiro, domingo de Páscoa, 1 de maio, 25 de dezembro", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "location" : { "address" : "Avenida de Brasília", "city" : "Lisbon", "country" : "Portugal", "coordinates" : "38.6915953567341 -9.21592950464517", "createdAt" : "Thu Sep 21 23