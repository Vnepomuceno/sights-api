{ "_id" : { "$oid" : "59c43d27dd5e3d0a02433b4c" }, "labels" : [ { "term" : "primary", "value" : "Miradouro do Castelo de São Jorge", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "descriptions" : [ { "value" : "O Castelo de São Jorge oferece uma vista privilegiada e muito completa da cidade. Deslizando os olhos pela Costa do Castelo, passamos pelos telhados da Mouraria até ao Martim Moniz. A contemplação pode prosseguir pela colina de Santana, pela Baixa e subir a São Roque, onde espreitam as ruínas do Convento do Carmo e o arvoredo do miradouro fronteiro de São Pedro de Alcântara. Percorrendo os telhados e as ruas estreitas e sinuosas de Alfama, passando pela Sé, os olhos avançam em direcção ao rio Tejo. Para Norte, avistamos as novas áreas da cidade, com avenidas largas e prédios de construção mais recente.\r\n", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "images" : [ { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-castelo_ASR2749-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-castelo_ASR2746-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-castelo_ASR2735-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-castelo_ASR2725-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "websites" : [ { "href" : "www.castelodesaojorge.pt", "type" : "text/html", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "contact" : { "phoneNumber" : "+351 218 800 620", "emailAddress" : "info@castelodesaojorge.pt" }, "schedules" : [ { "open" : "mar-out: 09h00 - 21h00\r\nnov-fev: 09h00 - 18h00", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "location" : { "address" : "Castelo de São Jorge", "city" : "Lisbon", "country" : "Portugal", "coordinates" : "38.7130085660449 -9.13421136351658", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, "citySdkId" : "52d7bf7d723e8e0b0cc09130", "base" : "http://tourism.citysdk.cm-lisboa.pt/pois/", "author" : { "source" : "open-data", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017" }, "createdAt" : "Fri Oct 19 15:32:34 WEST 2012", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" },
