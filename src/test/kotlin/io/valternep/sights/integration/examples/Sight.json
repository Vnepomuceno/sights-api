{ "_id" : { "$oid" : "59c43d2edd5e3d0a02433b5a" }, "labels" : [ { "term" : "primary", "value" : "Miradouro do Parque Eduardo VII de Inglaterra", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "term" : "note", "value" : "SG/DMC", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "descriptions" : [ { "value" : "Está integrado no PARQUE EDUARDO VII, e  tem uma vista que atravessa toda o vale da Avenida da Liberdade, a Baixa Pombalina e as encostas que a definem (à esquerda o Castelo de São Jorge, à direita as ruínas do Convento do Carmo), perdendo-se no leito do Tejo e, em dias de boa visibilidade, alcançando a outra margem.", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "images" : [ { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-parque-eduardo-VII_ASR2587.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/miradouro-parque-eduardo-VII_ASR2591.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "contact" : { "phoneNumber" : "+351 213 912 600", "emailAddress" : "daev@cm-lisboa.pt" }, "schedules" : [ { "open" : "24h", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "location" : { "address" : "Alameda Cardeal Cerejeira", "city" : "Lisbon", "country" : "Portugal", "coordinates" : "38.7303751081413 -9.15448160976616", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, "citySdkId" : "5605d24e723e8e0538bf297c", "base" : "http://tourism.citysdk.cm-lisboa.pt/pois/", "author" : { "source" : "open-data", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017" }, "crea