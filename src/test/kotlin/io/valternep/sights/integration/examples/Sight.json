{ "_id" : { "$oid" : "59c43d2cdd5e3d0a02433b56" }, "labels" : [ { "term" : "primary", "value" : "Miradouro do Monte Agudo", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "term" : "note", "value" : "SG-DMC", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "descriptions" : [ { "value" : "Com uma vista deslumbrante sobre a cidade, o Miradouro do Monte Agudo remonta aos anos 50 e estabelece uma ligação pedonal entre a Rua Heliodoro Salgado, onde se situa a entrada principal, e a Rua Ilha do Príncipe. Recentemente foi objeto de obras de reabilitação.\r\n\r\nÁrea: 1,2 ha\r\nEquipamentos: Pérgula; Painel de azulejos; Quiosque com esplanada", "lang" : "pt-PT", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "images" : [ { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/monte-agudo_ASR2711-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/MonteAgudo5.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/MonteAgudo3.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" }, { "href" : "http://www.cm-lisboa.pt/uploads/pics/tt_address/monte-agudo_ASR3120-2.jpg", "type" : "image/jpeg", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "websites" : [ { "href" : "http://www.cm-lisboa.pt/equipamentos/equipamento/info/miradouro-do-monte-agudo", "type" : "text/html", "createdAt" : "Thu Sep 21 23:28:43 WEST 2017", "updatedAt" : "Thu Sep 21 23:28:43 WEST 2017" } ], "contact" : { "phoneNumber" : "(351) 213 912 600", "